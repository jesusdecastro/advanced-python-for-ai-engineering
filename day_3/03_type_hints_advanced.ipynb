      "source": [
        "### Narrowing con match (Python 3.10+)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "def describe(value: int | str | list | dict) -> str:\n",
        "    \"\"\"Describe value using pattern matching.\"\"\"\n",
        "    \n",
        "    match value:\n",
        "        case int(x):\n",
        "            # x is int here\n",
        "            return f\"Integer: {x}\"\n",
        "        \n",
        "        case str(s):\n",
        "            # s is str here\n",
        "            return f\"String of length {len(s)}\"\n",
        "        \n",
        "        case list(items):\n",
        "            # items is list here\n",
        "            return f\"List with {len(items)} items\"\n",
        "        \n",
        "        case dict(d):\n",
        "            # d is dict here\n",
        "            return f\"Dict with {len(d)} keys\"\n",
        "        \n",
        "        case _:\n",
        "            return \"Unknown type\"\n",
        "\n",
        "print(describe(42))              # \"Integer: 42\"\n",
        "print(describe(\"hello\"))         # \"String of length 5\"\n",
        "print(describe([1, 2, 3]))       # \"List with 3 items\"\n",
        "print(describe({\"a\": 1, \"b\": 2})) # \"Dict with 2 keys\""
      ]
    },
