    "# Example 2: Processing API data with pagination\n",
    "def fetch_api_pages(base_url: str, total_pages: int):\n",
    "    \"\"\"\n",
    "    Fetch data from paginated API.\n",
    "    \n",
    "    :param base_url: Base URL of API\n",
    "    :type base_url: str\n",
    "    :param total_pages: Total number of pages\n",
    "    :type total_pages: int\n",
    "    :yield: Data from each page\n",
    "    :rtype: Generator[dict, None, None]\n",
    "    \"\"\"\n",
    "    for page in range(1, total_pages + 1):\n",
    "        # Simulated API call\n",
    "        print(f\"Fetching page {page}...\")\n",
    "        data = {\n",
    "            \"page\": page,\n",
    "            \"items\": [f\"item_{page}_{i}\" for i in range(3)]\n",
    "        }\n",
    "        yield data\n",
    "\n",
    "def process_all_items(api_generator):\n",
    "    \"\"\"\n",
    "    Process all items from API pages.\n",
    "    \n",
    "    :param api_generator: Generator yielding API pages\n",
    "    :return: List of all processed items\n",
    "    :rtype: list[str]\n",
    "    \"\"\"\n",
    "    all_items = []\n",
    "    for page_data in api_generator:\n",
    "        all_items.extend(page_data[\"items\"])\n",
    "    return all_items\n",
    "\n",
    "# Fetch and process API data\n",
    "items = process_all_items(fetch_api_pages(\"https://api.example.com\", 3))\n",
    "print(f\"\\nTotal items processed: {len(items)}\")\n",
    "print(f\"First 5 items: {items[:5]}\")"
