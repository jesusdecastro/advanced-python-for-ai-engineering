  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Normal function with return\n",
    "def get_squares_list(n: int) -> list[int]:\n",
    "    \"\"\"\n",
    "    Return list of squares from 0 to n-1.\n",
    "    \n",
    "    :param n: Upper limit\n",
    "    :type n: int\n",
    "    :return: List of squares\n",
    "    :rtype: list[int]\n",
    "    \"\"\"\n",
    "    result = []\n",
    "    for i in range(n):\n",
    "        result.append(i ** 2)\n",
    "    return result  # Returns everything at once\n",
    "\n",
    "# Generator function with yield\n",
    "def get_squares_generator(n: int):\n",
    "    \"\"\"\n",
    "    Generate squares from 0 to n-1 one at a time.\n",
    "    \n",
    "    :param n: Upper limit\n",
    "    :type n: int\n",
    "    :yield: Next square\n",
    "    :rtype: Generator[int, None, None]\n",
    "    \"\"\"\n",
    "    for i in range(n):\n",
    "        yield i ** 2  # Returns one value at a time\n",
    "\n",
    "# Compare behavior\n",
    "print(\"Normal function:\")\n",
    "squares_list = get_squares_list(5)\n",
    "print(f\"Type: {type(squares_list)}\")\n",
    "print(f\"Values: {squares_list}\")\n",
    "print()\n",
    "\n",
    "print(\"Generator function:\")\n",
    "squares_gen = get_squares_generator(5)\n",
    "print(f\"Type: {type(squares_gen)}\")\n",
    "print(f\"Values: {list(squares_gen)}\")"
   ]
