    "def read_file_lines(filepath: str):\n",
    "    \"\"\"\n",
    "    Read lines from a file.\n",
    "    \n",
    "    :param filepath: Path to file\n",
    "    :type filepath: str\n",
    "    :yield: Each line from file\n",
    "    :rtype: Generator[str, None, None]\n",
    "    \"\"\"\n",
    "    # Simulated file reading\n",
    "    lines = [f\"Line {i} from {filepath}\" for i in range(3)]\n",
    "    yield from lines\n",
    "\n",
    "def read_multiple_files(filepaths: list[str]):\n",
    "    \"\"\"\n",
    "    Read lines from multiple files.\n",
    "    \n",
    "    :param filepaths: List of file paths\n",
    "    :type filepaths: list[str]\n",
    "    :yield: Lines from all files\n",
    "    :rtype: Generator[str, None, None]\n",
    "    \"\"\"\n",
    "    for filepath in filepaths:\n",
    "        yield from read_file_lines(filepath)\n",
    "\n",
    "# Process multiple files\n",
    "files = [\"file1.txt\", \"file2.txt\"]\n",
    "for line in read_multiple_files(files):\n",
    "    print(line)"
